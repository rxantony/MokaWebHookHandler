{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "aws-sam",
            "request": "direct-invoke",
            "name": "common:com.mekari.mokaaddons.webhookhandler.common.event.EventValidator::validate (java11)",
            "invokeTarget": {
                "target": "code",
                "projectRoot": "${workspaceFolder}/common",
                "lambdaHandler": "com.mekari.mokaaddons.webhookhandler.common.event.EventValidator::validate"
            },
            "lambda": {
                "runtime": "java11",
                "payload": {},
                "environmentVariables": {}
            }
        },
        {
            "name": "Debug-Webhookhandler-api",
            "type": "java",
            "request": "launch",
            "externalConsole": false,
            "stopOnEntry": true,
            "console": "integratedTerminal",
            "preLaunchTask": "build-webhookhandler-api",
            "cwd": "${workspaceRoot}",
            "projectName": "mokaaddons-webhookhandler-api",
            "mainClass": "com.mekari.mokaaddons.webhookhandler.WebHookApiApplication"
        },
        {
            "name": "Debug-Webhookhandler-consumer",
            "type": "java",
            "request": "launch",
            "externalConsole": false,
            "stopOnEntry": true,
            "console": "integratedTerminal",
            "preLaunchTask": "build-webhookhandler-consumer",
            "cwd": "${workspaceRoot}",
            "projectName": "mokaaddons-webhookhandler-consumer",
            "mainClass": "com.mekari.mokaaddons.webhookhandler.WebHookConsumerApplication"
        }
    ],
    "compounds": [
        {
            "name": "Debug-Webhookhandler-consumer-api",
            "configurations": ["Debug-Webhookhandler-consumer", "Debug-Webhookhandler-api"]
        }
    ]
}